{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "ES2020",
	  "lib": ["ES2020", "DOM"],
	  "sourceMap": true,
	  "rootDir": "src",
	  "strict": true,
	  "skipLibCheck": true,
	  "esModuleInterop": true,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "baseUrl": "./src",
	},
	"exclude": [
	  "node_modules",
	  ".vscode-test",
	  ".external"
	]
  }
  